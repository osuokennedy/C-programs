#inlude <stdio.h>
#include <stdbool.h>
int main()
{
  // int - %d
  // unsigned int - %u
  // long long int - %lld
  // float - %f
  // char - %c
  // double - %lf
  // string - %c [Array of char]


  // %.1 = decimal precision
  //%1 = minimum field width
  // %-= left align

  float item1 = 5.05;
  float item2 = 10.25;
  float item3 = 100.50;

  printf("item1: $%f\n", Item1);
  printf("item2: $%f\n", Item2);
  printf("item3: $%f", item3);
  
}
